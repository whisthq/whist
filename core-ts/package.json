        "install": "^0.13.0",
        "jest": "^26.6.3",
        "jest-fetch-mock": "^3.0.3",
        "nodemon": "^2.0.7",
        "prettier": "^2.3.2",
        "rimraf": "^3.0.2",
        "ts-jest": "^26.5.5",
        "ts-node": "^10.0.0",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.3.5"
    },
    "dependencies": {
        "browser-or-node": "^1.3.0",
        "lodash": "^4.17.21",
        "node-fetch": "^2.6.1"
    },
    "scripts": {
        "compile": "tsc",
        "clean": "rimraf dist",
        "copy-package": "mkdir -p dist && cp package.json dist/package.json",
        "build": "npm run clean && npm run compile",
        "dev": "nodemon -r tsconfig-paths/register --watch src",
        "lint-check": "eslint src/** --no-error-on-unmatched-pattern",
        "lint-fix": "eslint --fix src/** --no-error-on-unmatched-pattern",
        "tsc-watch": "npm run clean && npm run compile -- --watch",
        "test": "npm run compile && jest",
        "test-watch": "jest --watch"
    }
